@using UmbracoProject.App_Code.Models
@model List<GlobalSearchModel>

@if (Model?.Count > 0)
{

    <ul class="list-group">
        @foreach (var item in Model)
        {
            <li class="list-group-item"><a href="@item?.Url">@item?.NodeName</a></li>
        }
    </ul>
}
else
{
    <ul class="list-group">
        <li class="list-group-item">No Record Found!!</li>
    </ul>
}